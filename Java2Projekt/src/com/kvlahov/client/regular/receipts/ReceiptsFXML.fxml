<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox prefHeight="388.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kvlahov.client.regular.receipts.ReceiptsFXMLController">
   <children>
      <BorderPane>
         <center>
            <TableView fx:id="tableView" prefHeight="314.0" prefWidth="378.0">
              <columns>
                <TableColumn fx:id="dateColumn" editable="false" prefWidth="150.0" text="Date" />
                <TableColumn fx:id="totalColumn" editable="false" prefWidth="75.0" text="Total" />
                  <TableColumn fx:id="createdByColumn" editable="false" prefWidth="150.0" text="Created by" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </TableView>
         </center>
      </BorderPane>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <JFXButton buttonType="RAISED" prefHeight="25.0" prefWidth="132.0" style="-fx-background-color: #FA8072;" text="Preview" />
         </children>
      </VBox>
      <AnchorPane fx:id="receiptPane">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </AnchorPane>
   </children>
</HBox>
